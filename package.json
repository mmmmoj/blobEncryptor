{
  "name": "blobencryptor",
  "version": "1.0.0",
  "description": "This library is used to enc/dec a blob file in fast way with javascript. It support most browsers and can work on Nodejs.",
  "main": "blobEncryptor.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "generate-docs": "node_modules/.bin/jsdoc blobEncryptor.js -t node_modules/docdash README.md",
    "minify": "uglifyjs blobEncryptor.js -c sequences=true,dead_code=true,conditionals=true,booleans=true,unused=true,if_return=true,join_vars=true,drop_console=true -m sort --screw-ie8 > blobEncryptor.min.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mmmmoj/blobEncryptor.git"
  },
  "keywords": [
    "blob",
    "encryption",
    "decryption",
    "javascript",
    "nodejs"
  ],
  "author": "M.Mohamed",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/mmmmoj/blobEncryptor/issues"
  },
  "homepage": "https://github.com/mmmmoj/blobEncryptor#readme",
  "dependencies": {
    "docdash": "^0.4.0",
    "jsdoc": "^3.4.2"
  },
  "devDependencies": {
    "uglify-js": "^2.4.23"
  }
}
